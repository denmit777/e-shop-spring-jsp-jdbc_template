<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<!DOCTYPE html>
<html>
    <head>
        <title>https://www.online-shop.com</title>
    </head>
    <body>
        <h2 align="center">You are unregistered user. Please, register right now</h2>
        <form action="register" align="center" method="post">
            <table align="center">
                <tr>
                    <td>Login</td>
                    <td><input type="text" name="login" placeholder="Enter your name"></td>
                </tr>
                <tr>
                    <td>Password</td>
                    <td><input type="password" name="password" placeholder="Enter your password"></td>
                </tr>
                <tr>
                    <td><input type="submit" value="Sign In"></td>
                </tr>
            </table>
        </form>
    </body>
</html>